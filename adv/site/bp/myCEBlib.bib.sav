@Article{Foreman.etal2011_JGRO,
  author   = {Foreman, M. G. G. and Pal, B. and Merryfield, W. J.},
  journal  = {J. Geophys. Res.: Oceans},
  title    = {Trends in upwelling and downwelling winds along the {British Columbia shelf}},
  year     = {2011},
  number   = {C10},
  volume   = {116},
  abstract = {Fifty-year time series combining observed and estimated winds at six buoys off the British Columbia coast are examined for trends in the timing and cumulative intensity of upwelling and downwelling. As with a similar analysis to the south, the onset of upwelling and the upwelling season duration are generally found to be arriving later in the year and becoming shorter, respectively. However, statistically significant trends toward increased cumulative upwelling and downwelling are found at most buoys and this is related to previous findings of trends in sea level pressure and winter mixed-layer depth in the northeast Pacific. Impacts on past and future local marine ecosystems are briefly discussed.},
  doi      = {10.1029/2011JC006995},
  eprint   = {https://agupubs.onlinelibrary.wiley.com/doi/pdf/10.1029/2011JC006995},
  file     = {:Articles/DFO_papers/Foreman.etal2011_JGR.pdf:PDF},
  groups   = {DFO-Pacific},
  keywords = {climate change, downwelling, upwelling},
}


@article{Forlini.etal2021_JGR,
author = {Forlini, Carola and Qayyum, Rizwan and Malej, Matt and Lam, Michael-Angelo Y.-H. and Shi, Fengyan and Angelini, Christine and Sheremet, Alex},
title = {On the Problem of Modeling the Boat Wake Climate: The Florida Intracoastal Waterway},
journal = {Journal of Geophysical Research: Oceans},
volume = {126},
number = {2},
pages = {e2020JC016676},
keywords = {boat wakes, numerical modeling, urbanized coastal waters, wake climate},
doi = {https://doi.org/10.1029/2020JC016676},
url = {https://agupubs.onlinelibrary.wiley.com/doi/abs/10.1029/2020JC016676},
eprint = {https://agupubs.onlinelibrary.wiley.com/doi/pdf/10.1029/2020JC016676},
note = {e2020JC016676 2020JC016676},
abstract = {Abstract Quantifying and forecasting the impact of boat traffic on the health of coastal ecosystems must account for the multiscale nature of the process: from minutes (individual wakes), to days (tidal phase), weeks, and longer (tide modulation, seasonal traffic). Direct numerical simulations covering all these scales are difficult, not in the least because specifying the vessel type and navigation characteristics for every wake is practically impossible. To overcome this problem, we propose a statistical-mechanics description of the wake field that focuses on classes of wakes, defined by a set of characteristic parameters, and their joint probability density in the characteristic-parameter space. Here, we demonstrate the first steps of the approach using existing numerical tools and parametrizations. Because vessel type and navigation characteristics are not available, an investigation of wake parameter space is not possible at this time. Instead, we use the leading-order Froude-number parametrization defined by a linear model to classify the wake population observed during the field experiment. Numerical tests applying the FUNWAVE-TVD model across all wake-classes identified show excellent skills for weakly dispersive wakes. The model is challenged by the short waves generated by small, slow boats. However, simulations suggest that the problem is confined to the deeper water domain and linear evolution. Nonlinear wake shoaling, essential for modeling wake-induced sediment transport and wake impact on the environment, is described well.},
year = {2021}
}


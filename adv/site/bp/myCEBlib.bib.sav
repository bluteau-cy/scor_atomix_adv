@Article{Mourino-Carballido.etal2021_SR,
  author   = {Mouri{\~n}o-Carballido, Beatriz and Otero Ferrer, Jos\'e Luis and Fern\'andez Castro, Bieito and Mara\~n\'on, Emilio and Blazquez Maseda, Mari{\~n}a and Aguiar-Gonz{\'a}lez, Borja and Chouci{\~n}o, Paloma and Gra{\~n}a, Roc\'io and Moreira-Coello, V\'ictor and Mari{\~n}a Villamana},
  title    = {Magnitude of nitrate turbulent diffusion in contrasting marine environments},
  journal  = {Sci. Rep.},
  year     = {2021},
  volume   = {11},
  number   = {1},
  pages    = {18804},
  month    = sep,
  issn     = {2045-2322},
  abstract = {Difficulties to quantify ocean turbulence have limited our knowledge about the magnitude and variability of nitrate turbulent diffusion, which constitutes one of the main processes responsible for the supply of nitrogen to phytoplankton inhabiting the euphotic zone. We use an extensive dataset of microturbulence observations collected in contrasting oceanic regions, to build a model for nitrate diffusion into the euphotic zone, and obtain the first global map for the distribution of this process. A model including two predictors (surface temperature and nitrate vertical gradient) explained 50\% of the variance in the nitrate diffusive flux. This model was applied to climatological data to predict nitrate diffusion in oligotrophic mid and low latitude regions. Mean nitrate diffusion ({\textasciitilde}â20 Tmol N yâ1) was comparable to nitrate entrainment due to seasonal mixed-layer deepening between 40Â°Nâ40ÂºS, and to the sum of global estimates of nitrogen fixation, fluvial fluxes and atmospheric deposition. These results indicate that nitrate diffusion represents one of the major sources of new nitrogen into the surface ocean in these regions.},
  doi      = {10.1038/s41598-021-97731-4},
}

